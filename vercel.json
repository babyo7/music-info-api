{
    "version": 2,
    "builds": [
      { "src": "server.js", "use": "@now/node" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/server.js" }
    ],
        "functions": {
          "api/src/functions/graphql.*": {
            "maxDuration": 120,
            "runtime": "@vercel/redwood@2.0.5"
          }
        }
  }
  